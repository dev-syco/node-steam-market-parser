{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      200
    ],
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-console": false
  },
  "rulesDirectory": []
}
